// Abstracts
///////////////////////////////////////////////////////////////////////////

// Point associated with water temperature
WaterTempPoint : NumberPoint <abstract> {
  water
  temp
  unit: Unit <quantity:"temperature"> "Â°F"
}

// Sensor for water temperature
WaterTempSensor : WaterTempPoint & SensorPoint <abstract>

// Setpoint for water temperature
WaterTempSp : WaterTempPoint & SpPoint <abstract>

//////////////////////////////////////////////////////////////////////////
// Chilled Water Temps
//////////////////////////////////////////////////////////////////////////

// Chilled Water Entering Temperature Sensor
ChilledWaterEnteringTempSensor : WaterTempSensor {
  chilled
  entering
}

// Chilled Water Leaving Temperature Sensor
ChilledWaterLeavingTempSensor : WaterTempSensor {
  chilled
  leaving
}

// Chilled Water Leaving Temperature Setpoint
ChilledWaterLeavingTempSp : WaterTempSp {
  chilled
  leaving
}
